    "frontend": {
      "root": "apps/frontend",
      "sourceRoot": "apps/frontend/src",
      "projectType": "application",
      "prefix": "frontend",
      "targets": {
        "build-deploy-test": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "npm run client-gen",
              "[ -f env/.env.dev ] || touch env/.env.dev",
              "cd apps/frontend && mkdir -p public/build && env-cmd -f ../../env/.env.dev $(npm bin)/webpack --node-env test",
              "mkdir -p dist/apps",
              "rm -r dist/apps/frontend || true",
              "cp -r apps/frontend/public/ dist/apps/frontend",
              "rm dist/apps/frontend/.gitignore"
            ],
            "parallel": false
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "cd apps/frontend && $(npm bin)/cross-env NODE_ENV=production $(npm bin)/webpack",
              "mkdir -p dist/apps",
              "rm -r dist/apps/frontend || true",
              "cp -r apps/frontend/public dist/apps/frontend"
            ]
          },
          "outputs": ["{options.outputPath}"],
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/frontend/src/environments/environment.ts",
                  "with": "apps/frontend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve-inner": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "cd apps/frontend && $(npm bin)/webpack serve"
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "[ -f env/.env.dev ] || touch env/.env.dev",
              "env-cmd -f env/.env.dev nx serve-inner frontend"
            ],
            "parallel": false
          }
        },
        "serve+api": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx serve api", "nx serve frontend"],
            "parallel": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/frontend/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "check": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "$(npm bin)/svelte-check",
            "cwd": "apps/frontend"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/frontend"],
          "options": {
            "jestConfig": "apps/frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
